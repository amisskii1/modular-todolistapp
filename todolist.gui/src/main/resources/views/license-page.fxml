        <Label layoutX="60.0" layoutY="118.0" prefHeight="26.0" prefWidth="164.0" text="Enter application license key:" />
        <TextField fx:id="email" layoutX="97.0" layoutY="83.0" prefHeight="26.0" prefWidth="189.0" promptText="Enter yours email" />
